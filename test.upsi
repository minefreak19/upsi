dim length; 
unit metre: length;  
dim time; 
unit second: time;
unit minute: time = 60 second; 
unit hour: time = 60 minute;

unit mile: length = 1.609344 * 1000 metre;

unit yard: length = ((1 mile) / 1760); 

let y: length;
let x: length = 1 mile; 
let z: length = 2 * x;

// This is a comment
y = z / 0.33;

print(y);
print(y in metre);
print((1 yard) in mile);
print(1 mile in yard);
print(y in yard);
print(1 hour in second);
print(1 second in hour);

